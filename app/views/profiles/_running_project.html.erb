<div id="running-project-<%= project.id%>">
  <div class="card card-project card-project-preview"  >
    <div class="row">
      <div class="col-sm-4">
        <div class="card-title" style="text-overflow: ellipsis;">
          <h3>
            <strong>
              <%= project.title %>
            </strong>
          </h3>
        </div>
      </div>
    </div>
    <div id="status-<%= project.id%>">
      <div class="progress">
        <div id="progress-<%= project.id%>" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:0%">
        </div>
      </div>
    </div>
    <div class="card-footer">
      <button onclick="return cancelProject(<%= project.id %>);" class="btn btn-warning cancel_project" id="cancel-<%= project.id%>" class="btn btn-danger"> Cancel </button>
    </div>
  </div>
</div>
